<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema targetNamespace="http://learnityframework.org/"
	elementFormDefault="qualified" xmlns="http://learnityframework.org/"
	xmlns:mstns="http://learnityframework.org/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">


	<xsd:simpleType name="NONEMPTY">
		<xsd:restriction base="xsd:token">
			<xsd:minLength value="1" />
		</xsd:restriction>
	</xsd:simpleType>


	<xsd:element name="interface">
		<xsd:complexType>
			<xsd:sequence>


				<xsd:element name="configuration" maxOccurs="1"
					minOccurs="0">
					<xsd:complexType>
						<xsd:attribute name="checkrole" use="optional">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:enumeration value="true" />
									<xsd:enumeration value="false" />
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
						<xsd:attribute name="createsession" use="optional">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:enumeration value="true" />
									<xsd:enumeration value="false" />
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
						<xsd:attribute type="NONEMPTY" name="TemplateID"
							use="optional" />
						<xsd:attribute type="NONEMPTY" name="ThemeID" use="optional" />
						<xsd:attribute name="Enable_Caching" use="optional">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:enumeration value="true" />
									<xsd:enumeration value="false" />
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
						<xsd:attribute type="NONEMPTY" name="CacheName" use="optional" />
						<xsd:attribute name="CacheDynamicJS" use="optional">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:enumeration value="true" />
									<xsd:enumeration value="false" />
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
						<xsd:attribute name="CacheDynamicCSS" use="optional">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:enumeration value="true" />
									<xsd:enumeration value="false" />
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
						<xsd:attribute name="CacheDynamicImage" use="optional">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:enumeration value="true" />
									<xsd:enumeration value="false" />
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="structure" maxOccurs="1" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="part" maxOccurs="unbounded"
								minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>


										<xsd:element name="option" maxOccurs="unbounded"
											minOccurs="0">
											<xsd:complexType>
												<xsd:attribute type="NONEMPTY" name="id" use="required" />
												<xsd:attribute type="NONEMPTY" name="name"
													use="required" />
												<xsd:attribute type="NONEMPTY" name="value"
													use="required" />
											</xsd:complexType>
										</xsd:element>

										<!-- An element can have everything which a part can have -->
										<xsd:element name="element" maxOccurs="unbounded"
											minOccurs="0">
											<xsd:complexType>
												<xsd:sequence>


													<xsd:element name="option" maxOccurs="unbounded"
														minOccurs="0">
														<xsd:complexType>
															<xsd:attribute type="NONEMPTY" name="id"
																use="required" />
															<xsd:attribute type="NONEMPTY" name="name"
																use="required" />
															<xsd:attribute type="NONEMPTY" name="value"
																use="required" />
														</xsd:complexType>
													</xsd:element>





													<xsd:element name="selector" maxOccurs="unbounded"
														minOccurs="0">
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="selectoroption"
																	maxOccurs="unbounded" minOccurs="0">
																	<xsd:complexType>
																		<xsd:attribute type="NONEMPTY" name="name"
																			use="required" />
																		<xsd:attribute type="NONEMPTY" name="value"
																			use="required" />
																	</xsd:complexType>
																</xsd:element>
															</xsd:sequence>
															<xsd:attribute type="NONEMPTY" name="id"
																use="required" />
															<xsd:attribute type="NONEMPTY" name="class"
																use="optional" />
															<xsd:attribute type="NONEMPTY" name="contentid"
																use="required" />
															<xsd:attribute type="NONEMPTY" name="behaviourid"
																use="required" />
															<xsd:attribute name="domaintype" use="required">
																<xsd:simpleType>
																	<xsd:restriction base="xsd:string">
																		<xsd:enumeration value="query" />
																		<xsd:enumeration value="fixed" />
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<!-- <xsd:attribute type="NONEMPTY" name="value" use="optional" 
																/> -->
															<xsd:attribute name="value" use="optional"
																type="NONEMPTY" />
															<xsd:attribute name="gridrefresh" use="optional">
																<xsd:simpleType>
																	<xsd:restriction base="xsd:string">
																		<xsd:enumeration value="true" />
																		<xsd:enumeration value="false" />
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute type="NONEMPTY" name="influence"
																use="optional" />
														</xsd:complexType>
													</xsd:element>


													<xsd:element name="column" maxOccurs="unbounded"
														minOccurs="0">
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="edit" maxOccurs="1"
																	minOccurs="0">
																	<xsd:complexType>
																		<!--<xsd:sequence> <xsd:element name=" " maxOccurs="unbounded" 
																			minOccurs="0"/> </xsd:sequence> -->
																		<xsd:attribute type="NONEMPTY" name="type"
																			use="optional" />
																		<xsd:attribute type="NONEMPTY" name="size"
																			use="optional" />
																		<xsd:attribute type="xsd:int" name="rows"
																			use="optional" />
																		<xsd:attribute name="cols" type="xsd:int"
																			use="optional" />
																		<xsd:attribute name="editdomaintype"
																			use="optional">
																			<xsd:simpleType>
																				<xsd:restriction base="xsd:string">
																					<xsd:enumeration value="true" />
																					<xsd:enumeration value="false" />
																					<xsd:enumeration value="query" />
																					<xsd:enumeration value="fixed" />
																				</xsd:restriction>
																			</xsd:simpleType>
																		</xsd:attribute>

																		<xsd:attribute name="multiple" use="optional">
																			<xsd:simpleType>
																				<xsd:restriction base="xsd:string">
																					<xsd:enumeration value="true" />
																					<xsd:enumeration value="false" />
																				</xsd:restriction>
																			</xsd:simpleType>
																		</xsd:attribute>

																		<xsd:attribute name="value" use="optional"
																			type="NONEMPTY" />
																	</xsd:complexType>
																</xsd:element>
															</xsd:sequence>
															<!-- <xsd:attribute type="NONEMPTY" name="head" use="optional" 
																/> -->
															<xsd:attribute name="head" use="optional"
																type="NONEMPTY" />
															<xsd:attribute type="NONEMPTY" name="influence"
																use="optional" />
															<xsd:attribute type="NONEMPTY" name="name"
																use="optional" />
															<xsd:attribute type="NONEMPTY" name="index"
																use="optional" />
															<xsd:attribute type="NONEMPTY" name="width"
																use="optional" />
															<xsd:attribute type="NONEMPTY" name="editable"
																use="optional" />
															<xsd:attribute type="NONEMPTY" name="default_type"
																use="optional" />
															<xsd:attribute type="NONEMPTY" name="default_value"
																use="optional" />
															<xsd:attribute type="NONEMPTY" name="hidden"
																use="optional" />
															<xsd:attribute type="NONEMPTY" name="edithidden"
																use="optional" />
															<xsd:attribute type="NONEMPTY" name="key"
																use="optional" />
															<xsd:attribute name="required" use="optional">
																<xsd:simpleType>
																	<xsd:restriction base="xsd:string">
																		<xsd:enumeration value="true" />
																		<xsd:enumeration value="false" />
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>

															<xsd:attribute name="email" use="optional">
																<xsd:simpleType>
																	<xsd:restriction base="xsd:string">
																		<xsd:enumeration value="true" />
																		<xsd:enumeration value="false" />
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>

															<xsd:attribute name="number" use="optional">
																<xsd:simpleType>
																	<xsd:restriction base="xsd:string">
																		<xsd:enumeration value="true" />
																		<xsd:enumeration value="false" />
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>


															<xsd:attribute name="custom" use="optional">
																<xsd:simpleType>
																	<xsd:restriction base="xsd:string">
																		<xsd:enumeration value="true" />
																		<xsd:enumeration value="false" />
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute type="NONEMPTY" name="custom_func"
																use="optional" />
															<xsd:attribute type="xsd:int" name="minval"
																use="optional" />
															<xsd:attribute type="xsd:int" name="maxval"
																use="optional" />
														</xsd:complexType>
													</xsd:element>

													<xsd:element name="select" maxOccurs="unbounded"
														minOccurs="0">
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="query" maxOccurs="unbounded"
																	minOccurs="0">
																	<xsd:complexType>
																		<xsd:attribute name="id" use="optional"
																			type="NONEMPTY" />
																		<xsd:attribute name="sql" use="required"
																			type="NONEMPTY" />
																		<xsd:attribute name="parameter" use="optional"
																			type="NONEMPTY" />
																	</xsd:complexType>
																</xsd:element>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="loadquery" maxOccurs="unbounded"
														minOccurs="0">
														<xsd:complexType>
															<xsd:attribute name="value" use="optional"
																type="NONEMPTY" />
															<!-- <xsd:attribute type="NONEMPTY" name="value" use="required" 
																/> -->
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="delete" maxOccurs="unbounded"
														minOccurs="0">
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="validations" maxOccurs="unbounded"
																	minOccurs="0">
																	<xsd:complexType>
																		<xsd:sequence>
																			<xsd:element name="validation" maxOccurs="unbounded"
																				minOccurs="0">
																				<xsd:complexType>
																					<xsd:sequence>
																						<xsd:element name="validationquery"
																							maxOccurs="unbounded" minOccurs="0">
																							<xsd:complexType>
																								<xsd:attribute type="xsd:int"
																									name="id" use="optional" />
																								<xsd:attribute name="sql" use="optional"
																									type="NONEMPTY" />
																								<xsd:attribute name="parameter"
																									use="optional" type="NONEMPTY" />
																								<xsd:attribute name="message" use="optional"
																									type="NONEMPTY" />
																								<!-- <xsd:attribute type="NONEMPTY" name="sql" use="optional" 
																									/> <xsd:attribute type="NONEMPTY" name="parameter" use="optional" /> <xsd:attribute 
																									type="NONEMPTY" name="message" use="optional" /> -->
																								<xsd:attribute type="NONEMPTY"
																									name="classname" use="optional" />
																							</xsd:complexType>
																						</xsd:element>
																					</xsd:sequence>
																					<xsd:attribute type="NONEMPTY" name="type"
																						use="optional" />
																				</xsd:complexType>
																			</xsd:element>
																		</xsd:sequence>
																	</xsd:complexType>
																</xsd:element>
																<xsd:element name="queries" maxOccurs="unbounded"
																	minOccurs="0">
																	<xsd:complexType>
																		<xsd:sequence>
																			<xsd:element name="query" maxOccurs="unbounded"
																				minOccurs="0">
																				<xsd:complexType>
																					<xsd:attribute type="xsd:int" name="id"
																						use="optional" />
																					<xsd:attribute name="sql" use="optional"
																						type="NONEMPTY" />
																					<xsd:attribute name="parameter" use="optional"
																						type="NONEMPTY" />
																				</xsd:complexType>
																			</xsd:element>
																		</xsd:sequence>
																	</xsd:complexType>
																</xsd:element>
																<xsd:element name="action" maxOccurs="unbounded"
																	minOccurs="0">
																	<xsd:complexType>
																		<xsd:attribute type="NONEMPTY" name="actionname"
																			use="required" />
																		<xsd:attribute name="sequence" use="required">
																			<xsd:simpleType>
																				<xsd:restriction base="xsd:string">
																					<xsd:enumeration value="before" />
																					<xsd:enumeration value="after" />
																					<xsd:enumeration value="replace" />
																				</xsd:restriction>
																			</xsd:simpleType>
																		</xsd:attribute>
																	</xsd:complexType>
																</xsd:element>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="add" maxOccurs="unbounded"
														minOccurs="0">
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="validations" maxOccurs="unbounded"
																	minOccurs="0">
																	<xsd:complexType>
																		<xsd:sequence>
																			<xsd:element name="validation" maxOccurs="unbounded"
																				minOccurs="0">
																				<xsd:complexType>
																					<xsd:sequence>
																						<xsd:element name="validationquery"
																							maxOccurs="unbounded" minOccurs="0">
																							<xsd:complexType>
																								<xsd:attribute type="xsd:int"
																									name="id" use="optional" />

																								<xsd:attribute name="sql" use="optional"
																									type="NONEMPTY" />
																								<xsd:attribute name="parameter"
																									use="optional" type="NONEMPTY" />
																								<xsd:attribute name="message" use="optional"
																									type="NONEMPTY" />


																								<!-- <xsd:attribute type="NONEMPTY" name="sql" use="optional" 
																									/> <xsd:attribute type="NONEMPTY" name="parameter" use="optional" /> <xsd:attribute 
																									type="NONEMPTY" name="message" use="optional" /> -->
																								<xsd:attribute type="NONEMPTY"
																									name="classname" use="optional" />
																							</xsd:complexType>
																						</xsd:element>
																					</xsd:sequence>
																					<xsd:attribute type="NONEMPTY" name="type"
																						use="optional" />
																				</xsd:complexType>
																			</xsd:element>
																		</xsd:sequence>
																	</xsd:complexType>
																</xsd:element>
																<xsd:element name="queries" maxOccurs="unbounded"
																	minOccurs="0">
																	<xsd:complexType>
																		<xsd:sequence>
																			<xsd:element name="query" maxOccurs="unbounded"
																				minOccurs="0">
																				<xsd:complexType>
																					<xsd:attribute type="xsd:int" name="id"
																						use="optional" />
																					<!-- <xsd:attribute type="NONEMPTY" name="sql" use="optional" 
																						/> <xsd:attribute type="NONEMPTY" name="parameter" use="optional" /> -->

																					<xsd:attribute name="sql" use="optional"
																						type="NONEMPTY" />
																					<xsd:attribute name="parameter" use="optional"
																						type="NONEMPTY" />

																				</xsd:complexType>
																			</xsd:element>
																		</xsd:sequence>
																	</xsd:complexType>
																</xsd:element>
																<xsd:element name="action" maxOccurs="unbounded"
																	minOccurs="0">
																	<xsd:complexType>
																		<xsd:attribute type="NONEMPTY" name="actionname"
																			use="required" />
																		<xsd:attribute name="sequence" use="required">
																			<xsd:simpleType>
																				<xsd:restriction base="xsd:string">
																					<xsd:enumeration value="before" />
																					<xsd:enumeration value="after" />
																					<xsd:enumeration value="replace" />
																				</xsd:restriction>
																			</xsd:simpleType>
																		</xsd:attribute>
																	</xsd:complexType>
																</xsd:element>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="modify" maxOccurs="unbounded"
														minOccurs="0">
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="validations" maxOccurs="unbounded"
																	minOccurs="0">
																	<xsd:complexType>
																		<xsd:sequence>
																			<xsd:element name="validation" maxOccurs="unbounded"
																				minOccurs="0">
																				<xsd:complexType>
																					<xsd:sequence>
																						<xsd:element name="validationquery"
																							maxOccurs="unbounded" minOccurs="0">
																							<xsd:complexType>
																								<xsd:attribute type="xsd:int"
																									name="id" use="optional" />
																								<xsd:attribute name="sql" use="optional"
																									type="NONEMPTY" />
																								<xsd:attribute name="parameter"
																									use="optional" type="NONEMPTY" />
																								<xsd:attribute name="message" use="optional"
																									type="NONEMPTY" />
																								<!-- <xsd:attribute type="NONEMPTY" name="sql" use="optional" 
																									/> <xsd:attribute type="NONEMPTY" name="parameter" use="optional" /> <xsd:attribute 
																									type="NONEMPTY" name="message" use="optional" /> -->
																								<xsd:attribute type="NONEMPTY"
																									name="classname" use="optional" />
																							</xsd:complexType>
																						</xsd:element>
																					</xsd:sequence>
																					<xsd:attribute type="NONEMPTY" name="type"
																						use="optional" />
																				</xsd:complexType>
																			</xsd:element>
																		</xsd:sequence>
																	</xsd:complexType>
																</xsd:element>
																<xsd:element name="queries" maxOccurs="unbounded"
																	minOccurs="0">
																	<xsd:complexType>
																		<xsd:sequence>
																			<xsd:element name="query" maxOccurs="unbounded"
																				minOccurs="0">
																				<xsd:complexType>
																					<xsd:attribute type="NONEMPTY" name="id"
																						use="optional" />
																					<xsd:attribute name="sql" use="optional"
																						type="NONEMPTY" />
																					<xsd:attribute name="parameter" use="optional"
																						type="NONEMPTY" />

																					<!-- <xsd:attribute type="NONEMPTY" name="sql" use="optional" 
																						/> <xsd:attribute type="NONEMPTY" name="parameter" use="optional" /> -->
																				</xsd:complexType>
																			</xsd:element>
																		</xsd:sequence>
																	</xsd:complexType>
																</xsd:element>
																<xsd:element name="action" maxOccurs="unbounded"
																	minOccurs="0">
																	<xsd:complexType>
																		<xsd:attribute type="NONEMPTY" name="actionname"
																			use="required" />
																		<xsd:attribute name="sequence" use="required">
																			<xsd:simpleType>
																				<xsd:restriction base="xsd:string">
																					<xsd:enumeration value="before" />
																					<xsd:enumeration value="after" />
																					<xsd:enumeration value="replace" />
																				</xsd:restriction>
																			</xsd:simpleType>
																		</xsd:attribute>
																	</xsd:complexType>
																</xsd:element>

															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
												</xsd:sequence>
												<!--<xsd:simpleContent> <xsd:extension base="xsd:string"> -->
												<xsd:attribute type="NONEMPTY" name="id" use="required" />
												<xsd:attribute name="autocomplete" use="optional">
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:enumeration value="on" />
															<xsd:enumeration value="off" />
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:attribute>
