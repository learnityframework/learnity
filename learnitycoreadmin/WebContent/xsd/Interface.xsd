<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema targetNamespace="http://learnityframework.org/"
	elementFormDefault="qualified" xmlns="http://learnityframework.org/"
	xmlns:mstns="http://learnityframework.org/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:element name="interface">
		<xsd:complexType>
			<xsd:sequence>


				<xsd:element name="configuration" maxOccurs="1"
					minOccurs="0">
					<xsd:complexType>
						<xsd:attribute name="checkrole" use="optional">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:enumeration value="true" />
									<xsd:enumeration value="false" />
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
						<xsd:attribute name="createsession" use="optional">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:enumeration value="true" />
									<xsd:enumeration value="false" />
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
						<xsd:attribute type="xsd:NMTOKEN" name="TemplateID"
							use="optional" />
						<xsd:attribute type="xsd:NMTOKEN" name="ThemeID"
							use="optional" />
						<xsd:attribute name="Enable_Caching" use="optional">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:enumeration value="true" />
									<xsd:enumeration value="false" />
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
						<xsd:attribute type="xsd:NMTOKEN" name="CacheName"
							use="optional" />
						<xsd:attribute name="CacheDynamicJS" use="optional">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:enumeration value="true" />
									<xsd:enumeration value="false" />
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
						<xsd:attribute name="CacheDynamicCSS" use="optional">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:enumeration value="true" />
									<xsd:enumeration value="false" />
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
						<xsd:attribute name="CacheDynamicImage" use="optional">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:enumeration value="true" />
									<xsd:enumeration value="false" />
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="structure" maxOccurs="1" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="part" maxOccurs="unbounded"
								minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>


										<xsd:element name="option" maxOccurs="unbounded"
											minOccurs="0">
											<xsd:complexType>
												<xsd:attribute type="xsd:NMTOKEN" name="id"
													use="required" />
												<xsd:attribute type="xsd:NMTOKEN" name="name"
													use="required" />
												<xsd:attribute type="xsd:NMTOKEN" name="value"
													use="required" />
											</xsd:complexType>
										</xsd:element>



										<xsd:element name="selector" maxOccurs="unbounded"
											minOccurs="0">
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="selectoroption" maxOccurs="unbounded"
														minOccurs="0">
														<xsd:complexType>
															<xsd:attribute type="xsd:NMTOKEN" name="name"
																use="required" />
															<xsd:attribute type="xsd:NMTOKEN" name="value"
																use="required" />
														</xsd:complexType>
													</xsd:element>
												</xsd:sequence>
												<xsd:attribute type="xsd:NMTOKEN" name="id"
													use="required" />
												<xsd:attribute type="xsd:NMTOKEN" name="class"
													use="optional" />
												<xsd:attribute type="xsd:NMTOKEN" name="contentid"
													use="required" />
												<xsd:attribute type="xsd:NMTOKEN" name="behaviourid"
													use="required" />
												<xsd:attribute name="domaintype" use="required">
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:enumeration value="query" />
															<xsd:enumeration value="fixed" />
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:attribute>
												<!-- <xsd:attribute type="xsd:NMTOKEN" name="value" use="optional" 
													/> -->
												<xsd:attribute name="value" use="optional">
													<xsd:simpleType>
														<xsd:restriction base="xsd:token">
															<xsd:minLength value="1" />
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:attribute>
												<xsd:attribute name="gridrefresh" use="optional">
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:enumeration value="true" />
															<xsd:enumeration value="false" />
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:attribute>
												<xsd:attribute type="xsd:NMTOKEN" name="influence"
													use="optional" />
											</xsd:complexType>
										</xsd:element>


										<xsd:element name="column" maxOccurs="unbounded"
											minOccurs="0">
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="edit" maxOccurs="1"
														minOccurs="0">
														<xsd:complexType>
															<!--<xsd:sequence> <xsd:element name=" " maxOccurs="unbounded" 
																minOccurs="0"/> </xsd:sequence> -->
															<xsd:attribute type="xsd:NMTOKEN" name="type"
																use="optional" />
															<xsd:attribute type="xsd:NMTOKEN" name="size"
																use="optional" />
															<xsd:attribute type="xsd:int" name="rows"
																use="optional" />
															<xsd:attribute name="cols" type="xsd:int"
																use="optional" />
															<xsd:attribute name="editdomaintype" use="optional">
																<xsd:simpleType>
																	<xsd:restriction base="xsd:string">
																		<xsd:enumeration value="true" />
																		<xsd:enumeration value="false" />
																		<xsd:enumeration value="query" />
																		<xsd:enumeration value="fixed" />
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>

															<xsd:attribute name="multiple" use="optional">
																<xsd:simpleType>
																	<xsd:restriction base="xsd:string">
																		<xsd:enumeration value="true" />
																		<xsd:enumeration value="false" />
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>

															<xsd:attribute name="value" use="optional">
																<xsd:simpleType>
																	<xsd:restriction base="xsd:token">
																		<xsd:minLength value="1" />
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
														</xsd:complexType>
													</xsd:element>
												</xsd:sequence>
												<!-- <xsd:attribute type="xsd:NMTOKEN" name="head" use="optional" 
													/> -->
												<xsd:attribute name="head" use="optional">
													<xsd:simpleType>
														<xsd:restriction base="xsd:token">
															<xsd:minLength value="1" />
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:attribute>
												<xsd:attribute type="xsd:NMTOKEN" name="influence"
													use="optional" />
												<xsd:attribute type="xsd:NMTOKEN" name="name"
													use="optional" />
												<xsd:attribute type="xsd:NMTOKEN" name="index"
													use="optional" />
												<xsd:attribute type="xsd:NMTOKEN" name="width"
													use="optional" />
												<xsd:attribute type="xsd:NMTOKEN" name="editable"
													use="optional" />
												<xsd:attribute type="xsd:NMTOKEN" name="hidden"
													use="optional" />
												<xsd:attribute type="xsd:NMTOKEN" name="edithidden"
													use="optional" />
												<xsd:attribute type="xsd:NMTOKEN" name="key"
													use="optional" />
												<xsd:attribute name="required" use="optional">
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:enumeration value="true" />
															<xsd:enumeration value="false" />
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:attribute>
												<xsd:attribute name="custom" use="optional">
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:enumeration value="true" />
															<xsd:enumeration value="false" />
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:attribute>
												<xsd:attribute type="xsd:NMTOKEN" name="custom_func"
													use="optional" />
												<xsd:attribute type="xsd:int" name="minval"
													use="optional" />
												<xsd:attribute type="xsd:int" name="maxval"
													use="optional" />
											</xsd:complexType>
										</xsd:element>
										<xsd:element name="loadquery" maxOccurs="unbounded"
											minOccurs="0">
											<xsd:complexType>
												<xsd:attribute name="value" use="optional">
													<xsd:simpleType>
														<xsd:restriction base="xsd:token">
															<xsd:minLength value="1" />
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:attribute>
												<!-- <xsd:attribute type="xsd:NMTOKEN" name="value" use="required" 
													/> -->
											</xsd:complexType>
										</xsd:element>
										<xsd:element name="delete" maxOccurs="unbounded"
											minOccurs="0">
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="validations" maxOccurs="unbounded"
														minOccurs="0">
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="validation" maxOccurs="unbounded"
																	minOccurs="0">
																	<xsd:complexType>
																		<xsd:sequence>
																			<xsd:element name="validationquery"
																				maxOccurs="unbounded" minOccurs="0">
																				<xsd:complexType>
																					<xsd:attribute type="xsd:int" name="id"
																						use="optional" />
																					<xsd:attribute name="sql" use="optional">
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:token">
																								<xsd:minLength value="1" />
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="parameter" use="optional">
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:token">
																								<xsd:minLength value="1" />
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="message" use="optional">
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:token">
																								<xsd:minLength value="1" />
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<!-- <xsd:attribute type="xsd:NMTOKEN" name="sql" use="optional" 
																						/> <xsd:attribute type="xsd:NMTOKEN" name="parameter" use="optional" /> <xsd:attribute 
																						type="xsd:NMTOKEN" name="message" use="optional" /> -->
																					<xsd:attribute type="xsd:NMTOKEN"
																						name="classname" use="optional" />
																				</xsd:complexType>
																			</xsd:element>
																		</xsd:sequence>
																		<xsd:attribute type="xsd:NMTOKEN" name="type"
																			use="optional" />
																	</xsd:complexType>
																</xsd:element>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="queries" maxOccurs="unbounded"
														minOccurs="0">
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="query" maxOccurs="unbounded"
																	minOccurs="0">
																	<xsd:complexType>
																		<xsd:attribute type="xsd:int" name="id"
																			use="optional" />
																		<xsd:attribute name="sql" use="optional">
																			<xsd:simpleType>
																				<xsd:restriction base="xsd:token">
																					<xsd:minLength value="1" />
																				</xsd:restriction>
																			</xsd:simpleType>
																		</xsd:attribute>
																		<xsd:attribute name="parameter" use="optional">
																			<xsd:simpleType>
																				<xsd:restriction base="xsd:token">
																					<xsd:minLength value="1" />
																				</xsd:restriction>
																			</xsd:simpleType>
																		</xsd:attribute>
																	</xsd:complexType>
																</xsd:element>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="action" maxOccurs="unbounded"
														minOccurs="0">
														<xsd:complexType>
															<xsd:attribute type="xsd:NMTOKEN" name="actionname"
																use="required" />
															<xsd:attribute name="sequence" use="required">
																<xsd:simpleType>
																	<xsd:restriction base="xsd:string">
																		<xsd:enumeration value="before" />
																		<xsd:enumeration value="after" />
																		<xsd:enumeration value="replace" />
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
														</xsd:complexType>
													</xsd:element>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
										<xsd:element name="add" maxOccurs="unbounded"
											minOccurs="0">
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="validations" maxOccurs="unbounded"
														minOccurs="0">
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="validation" maxOccurs="unbounded"
																	minOccurs="0">
																	<xsd:complexType>
																		<xsd:sequence>
																			<xsd:element name="validationquery"
																				maxOccurs="unbounded" minOccurs="0">
																				<xsd:complexType>
																					<xsd:attribute type="xsd:int" name="id"
																						use="optional" />

																					<xsd:attribute name="sql" use="optional">
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:token">
																								<xsd:minLength value="1" />
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="parameter" use="optional">
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:token">
																								<xsd:minLength value="1" />
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="message" use="optional">
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:token">
																								<xsd:minLength value="1" />
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>


																					<!-- <xsd:attribute type="xsd:NMTOKEN" name="sql" use="optional" 
																						/> <xsd:attribute type="xsd:NMTOKEN" name="parameter" use="optional" /> <xsd:attribute 
																						type="xsd:NMTOKEN" name="message" use="optional" /> -->
																					<xsd:attribute type="xsd:NMTOKEN"
																						name="classname" use="optional" />
																				</xsd:complexType>
																			</xsd:element>
																		</xsd:sequence>
																		<xsd:attribute type="xsd:NMTOKEN" name="type"
																			use="optional" />
																	</xsd:complexType>
																</xsd:element>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="queries" maxOccurs="unbounded"
														minOccurs="0">
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="query" maxOccurs="unbounded"
																	minOccurs="0">
																	<xsd:complexType>
																		<xsd:attribute type="xsd:int" name="id"
																			use="optional" />
																		<!-- <xsd:attribute type="xsd:NMTOKEN" name="sql" use="optional" 
																			/> <xsd:attribute type="xsd:NMTOKEN" name="parameter" use="optional" /> -->

																		<xsd:attribute name="sql" use="optional">
																			<xsd:simpleType>
																				<xsd:restriction base="xsd:token">
																					<xsd:minLength value="1" />
																				</xsd:restriction>
																			</xsd:simpleType>
																		</xsd:attribute>
																		<xsd:attribute name="parameter" use="optional">
																			<xsd:simpleType>
																				<xsd:restriction base="xsd:token">
																					<xsd:minLength value="1" />
																				</xsd:restriction>
																			</xsd:simpleType>
																		</xsd:attribute>

																	</xsd:complexType>
																</xsd:element>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="action" maxOccurs="unbounded"
														minOccurs="0">
														<xsd:complexType>
															<xsd:attribute type="xsd:NMTOKEN" name="actionname"
																use="required" />
															<xsd:attribute name="sequence" use="required">
																<xsd:simpleType>
																	<xsd:restriction base="xsd:string">
																		<xsd:enumeration value="before" />
																		<xsd:enumeration value="after" />
																		<xsd:enumeration value="replace" />
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
														</xsd:complexType>
													</xsd:element>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
										<xsd:element name="modify" maxOccurs="unbounded"
											minOccurs="0">
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="validations" maxOccurs="unbounded"
														minOccurs="0">
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="validation" maxOccurs="unbounded"
																	minOccurs="0">
																	<xsd:complexType>
																		<xsd:sequence>
																			<xsd:element name="validationquery"
																				maxOccurs="unbounded" minOccurs="0">
																				<xsd:complexType>
																					<xsd:attribute type="xsd:int" name="id"
																						use="optional" />
																					<xsd:attribute name="sql" use="optional">
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:token">
																								<xsd:minLength value="1" />
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="parameter" use="optional">
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:token">
																								<xsd:minLength value="1" />
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="message" use="optional">
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:token">
																								<xsd:minLength value="1" />
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<!-- <xsd:attribute type="xsd:NMTOKEN" name="sql" use="optional" 
																						/> <xsd:attribute type="xsd:NMTOKEN" name="parameter" use="optional" /> <xsd:attribute 
																						type="xsd:NMTOKEN" name="message" use="optional" /> -->
																					<xsd:attribute type="xsd:NMTOKEN"
																						name="classname" use="optional" />
																				</xsd:complexType>
																			</xsd:element>
																		</xsd:sequence>
																		<xsd:attribute type="xsd:NMTOKEN" name="type"
																			use="optional" />
																	</xsd:complexType>
																</xsd:element>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="queries" maxOccurs="unbounded"
														minOccurs="0">
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="query" maxOccurs="unbounded"
																	minOccurs="0">
																	<xsd:complexType>
																		<xsd:attribute type="xsd:NMTOKEN" name="id"
																			use="optional" />
																		<xsd:attribute name="sql" use="optional">
																			<xsd:simpleType>
																				<xsd:restriction base="xsd:token">
																					<xsd:minLength value="1" />
																				</xsd:restriction>
																			</xsd:simpleType>
																		</xsd:attribute>
																		<xsd:attribute name="parameter" use="optional">
																			<xsd:simpleType>
																				<xsd:restriction base="xsd:token">
																					<xsd:minLength value="1" />
																				</xsd:restriction>
																			</xsd:simpleType>
																		</xsd:attribute>

																		<!-- <xsd:attribute type="xsd:NMTOKEN" name="sql" use="optional" 
																			/> <xsd:attribute type="xsd:NMTOKEN" name="parameter" use="optional" /> -->
																	</xsd:complexType>
																</xsd:element>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="action" maxOccurs="unbounded"
														minOccurs="0">
														<xsd:complexType>
															<xsd:attribute type="xsd:NMTOKEN" name="actionname"
																use="required" />
															<xsd:attribute name="sequence" use="required">
																<xsd:simpleType>
																	<xsd:restriction base="xsd:string">
																		<xsd:enumeration value="before" />
																		<xsd:enumeration value="after" />
																		<xsd:enumeration value="replace" />
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
														</xsd:complexType>
													</xsd:element>

												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
									<!--<xsd:simpleContent> <xsd:extension base="xsd:string"> -->
									<xsd:attribute type="xsd:NMTOKEN" name="id" use="required" />
									<xsd:attribute name="autocomplete" use="optional">
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:enumeration value="on" />
												<xsd:enumeration value="off" />
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:attribute>
									<xsd:attribute name="class" use="required">
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:enumeration value="label" />
												<xsd:enumeration value="image" />
												<xsd:enumeration value="textlink" />
												<xsd:enumeration value="list" />
												<xsd:enumeration value="listitem" />
												<xsd:enumeration value="inputtext" />
												<xsd:enumeration value="textarea" />
												<xsd:enumeration value="combo" />
												<xsd:enumeration value="button" />
												<xsd:enumeration value="form" />
												<xsd:enumeration value="iframe" />
												<xsd:enumeration value="ajaxcomponent" />
												<xsd:enumeration value="applet" />
												<xsd:enumeration value="DBgrid" />
